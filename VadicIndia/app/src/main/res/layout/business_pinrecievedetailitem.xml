<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/expanselistlayout"

    >
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="5dp"
        app:cardElevation="2dp"
        app:contentPadding="5dp"
        android:id="@+id/pin_receive_detail_item_cardView_item"
        app:cardPreventCornerOverlap="true"
        android:layout_margin="5dp">

        /*Linear Layout PinReceive Detail Item*/
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:orientation="vertical">


            *Linear Layout for TextView S.No*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="S.No"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_sno"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Transfer From IE*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="Transfer From IE"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_fromId"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Transfer From Entrepreneur*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="Transfer From Entrepreneur"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_fromMember"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Product Name*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="Product Name"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_product_name"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Pin No*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="Pin No"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_pinNo"
                    >
                </TextView>

            </LinearLayout>
            /*Linear Layout for TextView Scratch No*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="Scratch No"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_scratchNo"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Pin Date*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="Date"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_date"
                    >
                </TextView>

            </LinearLayout>
            /*Linear Layout for TextView Status */
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text="Status"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:text=":"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/pin_receive_detail_item_txtView_status"
                    >
                </TextView>

            </LinearLayout>

        </LinearLayout>

    </androidx.cardview.widget.CardView>
</LinearLayout>