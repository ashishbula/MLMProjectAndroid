<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_margin="@dimen/margin_v_small"
    android:id="@+id/expanselistlayout">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="@dimen/margin_v_small"
        app:cardElevation="@dimen/margin_v_small"
        app:contentPadding="@dimen/margin_v_small"
        android:id="@+id/level_direct_item_cardView_item"
        app:cardPreventCornerOverlap="true"
        android:layout_margin="@dimen/margin_v_small">

        /*Linear Layout Repurchase incentive Item*/
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="0dp"
            android:orientation="vertical">


            *Linear Layout for TextView S no.*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/white_bg_box_black_border"
                android:visibility="visible"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="S.No."
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.6"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_sno"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Level No*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/white_bg_box_black_border"
                android:orientation="horizontal"
                android:padding="@dimen/margin_v_small"
                android:visibility="gone"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/level"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:text=""
                    android:textColor="@color/black"
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_level"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Id No*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/margin_v_small"
                android:padding="@dimen/margin_v_small"
                android:background="@drawable/white_bg_box_black_border"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/str_idno"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_idno"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Member Name*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/white_bg_box_black_border"
                android:padding="@dimen/margin_v_small"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/member_name"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_mem_name"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Sponsor ID*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/white_bg_box_black_border"
                android:padding="@dimen/margin_v_small"
                android:layout_marginTop="@dimen/margin_v_small"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/sponsor_id"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_sponsor_id"
                    >
                </TextView>

            </LinearLayout>
            /*Linear Layout for TextView Sponsor Name*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/white_bg_box_black_border"
                android:padding="@dimen/margin_v_small"
                android:id="@+id/level_direct_item_layout_spons_name"
                android:layout_marginTop="@dimen/margin_v_small"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/str_sponser_nme"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_sponsor_name"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView BV*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/level_direct_item_layout_bv"
                android:background="@drawable/white_bg_box_black_border"
                android:padding="@dimen/margin_v_small"
                android:layout_marginTop="@dimen/margin_v_small"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/str_bv"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_bv"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Package name*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/white_bg_box_black_border"
                android:padding="@dimen/margin_v_small"
                android:id="@+id/level_direct_item_layout_pkg"
                android:layout_marginTop="@dimen/margin_v_small"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/packg_name"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_pckg"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Active Status*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/white_bg_box_black_border"
                android:padding="@dimen/margin_v_small"
                android:id="@+id/level_direct_item_layout_status"
                android:layout_marginTop="@dimen/margin_v_small"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/activate_status"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_status"
                    >
                </TextView>

            </LinearLayout>

            /*Linear Layout for TextView Active Date*/
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/white_bg_box_black_border"
                android:padding="@dimen/margin_v_small"
                android:id="@+id/level_direct_item_layout_date"
                android:layout_marginTop="@dimen/margin_v_small"
                android:orientation="horizontal"
                android:weightSum="3">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.2"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text="@string/str_activate_date"
                    android:layout_height="wrap_content">
                </TextView>
                <!--Line-->
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/black"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight=".3"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:text=""
                    android:layout_height="wrap_content">
                </TextView>
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:textSize="@dimen/txt_size_normal"
                    android:textColor="@color/black"
                    android:layout_height="wrap_content"
                    android:id="@+id/level_direct_item_txt_activeDate"
                    >
                </TextView>

            </LinearLayout>

        </LinearLayout>

    </androidx.cardview.widget.CardView>
</LinearLayout>